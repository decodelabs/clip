{
    "name": "decodelabs/clip",
    "description": "CLI kernel integration for DecodeLabs Genesis",
    "type": "library",
    "keywords": [ ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",
        "composer-runtime-api": "^2.2",

        "decodelabs/archetype": "^0.3.8",
        "decodelabs/atlas": "^0.12.3",
        "decodelabs/coercion": "^0.3",
        "decodelabs/dictum": "^0.6.9",
        "decodelabs/exceptional": "^0.5.2",
        "decodelabs/fluidity": "^0.3.6",
        "decodelabs/genesis": "^0.11",
        "decodelabs/glitch": "^0.19",
        "decodelabs/terminus": "^0.11"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Clip\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DecodeLabs\\Clip\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/clip"
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
